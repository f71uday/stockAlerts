	<http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="db1c32a9-ad75-4516-8be2-ceb7a2fcd0ce" >
		<http:listener-connection host="0.0.0.0" port="8081" />
	</http:listener-config>
	<http:request-config name="HTTP_Request_configuration" doc:name="HTTP Request configuration" doc:id="99ceaff2-5641-4319-8512-6376163309f3" >
		<http:request-connection protocol="HTTPS" host="${api.host}" />
	</http:request-config>
	<configuration-properties doc:name="Configuration properties" doc:id="7b77ecfa-9d71-49c1-9a23-5952cddcd2b8" file="config.yaml" />
	<sns:config name="Amazon_SNS_configuration" doc:name="Amazon SNS configuration" doc:id="344b73c2-a876-431a-9ddf-4417a4455902" >
		<sns:basic-connection testTopic="${sns.topic}" accessKey="${secure::sns.accessKey}" secretKey="${secure::sns.secretKey}" />
	</sns:config>
	<validation:config name="Validation_Config" doc:name="Validation Config" doc:id="18260ebd-9f8d-45fe-a05b-9781c80ee0ec" />
	<os:object-store name="Object_store" doc:name="Object store" doc:id="4e989161-2f56-4ab5-be0c-7f13bc67d7f4" />
