		<twilio:account-sid-auth-token-connection username="${secure::twilio.username}" password="${secure::twilio.password}"/>
	</twilio:config>
	<api-gateway:autodiscovery apiId="17580089" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="6a9d35a2-bdfe-428b-a40b-cc46d880ea8a" flowRef="stockalert-main" />
	<secure-properties:config name="Secure_Properties_Config" doc:name="Secure Properties Config" doc:id="066a83fe-049e-4159-8b7e-31303f5b55a4" file="config.yaml" key="mulesoft" >
		<secure-properties:encrypt algorithm="Blowfish" />
	</secure-properties:config>
